<?xml version="1.0" encoding="utf-8" standalone="no"?>
<!DOCTYPE document SYSTEM "rml.dtd">
<document filename="certificate.pdf"
	  invariant="1"
	  compression="1"
	  creator="NextThought"
	  author="NextThought"
	  xmlns:tal="http://xml.zope.org/namespaces/tal"
	  xmlns:metal="http://xml.zope.org/namespaces/metal"
	  tal:attributes="title view/Title"
	  metal:define-macro="certificate"
	  tal:define="branding_graphic python: options['CertificateSideBarImage'];
                  brand_color python: options['BrandColor'];
                  brand_logo python: options['BrandLogo']">

  <docinit>
    <registerTTFont faceName="OpenSans" fileName="[nti.app.products.courseware]/completion/templates/fonts/NotoSansNextThought-Regular.ttf" />
    <registerTTFont faceName="OpenSansSemiBold" fileName="[nti.app.products.courseware]/completion/templates/fonts/NotoSansNextThought-SemiBold.ttf" />
    <registerTTFont faceName="OpenSansBold" fileName="[nti.app.products.courseware]/completion/templates/fonts/NotoSansNextThought-Bold.ttf" />
    <registerTTFont faceName="OpenSansLight" fileName="[nti.app.products.courseware]/completion/templates/fonts/NotoSansNextThought-Light.ttf" />
  </docinit>

  <template pagesize="(792,612)" leftMargin="52" showBoundary="0">
    <pageTemplate id="landscape">
      <pageGraphics>
          <tal:block metal:use-macro="macro:sidebar-fill" />
          <image metal:use-macro="macro:certificate-branding-graphic" file="[nti.app.products.courseware]/completion/templates/assets/sidebar-graphic.png" x="29" y="0" height="612" width="187"/>
          <image metal:use-macro="macro:top-right-branding-graphic" file="[nti.app.products.courseware]/completion/templates/assets/sidebar-graphic.png" x="0" y="0" height="0" width="0"/>
      </pageGraphics>
      <frame id="logo" x1="29" y1="0" width="187" height="612" />
      <frame id="heading" x1="238" y1="20" width="533" height="540"/>
    </pageTemplate>
  </template>

  <stylesheet>
	<blockTableStyle id="simple">
		<blockAlignment value="center"/>
		<blockValign value="middle"/>
	</blockTableStyle>
	<paraStyle name="header" fontName="OpenSans" fontSize="13"
               leftIndent="18" rightIndent="18" spaceBefore="41" leading="17" textColor="#494949"/>
    <paraStyle name="name" fontName="OpenSansLight" fontSize="40" leftIndent="18"
               rightIndent="18" spaceBefore="4" leading="55" textColor="#494949" />
    <paraStyle name="course" fontName="OpenSans" fontSize="23" leftIndent="18" rightIndent="18"
               spaceBefore="4" leading="31" textColor="#494949" />
    <paraStyle name="license_line" fontName="OpenSansSemiBold" fontSize="8" textColor="#838383"
               spaceBefore="7" leading="14" leftIndent="18"/>
    <paraStyle name="date" fontName="OpenSans" fontSize="13" leftIndent="18" rightIndent="18"
               spaceBefore="20" leading="17" textColor="#9C9C9C" />
    <paraStyle name="credit" fontName="OpenSansSemiBold" fontSize="10" leftIndent="18" rightIndent="18"
               spaceBefore="7" leading="14" textColor="#494949" />
    <paraStyle name="section-header" fontName="OpenSansSemiBold" fontSize="8" leftIndent="18" rightIndent="170"
               spaceBefore="7" leading="14" textColor="#494949" />
    <paraStyle name="section-info" fontName="OpenSans" fontSize="7" leftIndent="18" rightIndent="170"
               spaceBefore="2" leading="12" textColor="#757474" />
    <paraStyle name="facilitator-name" fontName="OpenSansSemiBold" fontSize="10" leftIndent="18" rightIndent="18" spaceBefore="7" leading="14" textColor="#494949" />
    <paraStyle name="facilitator-title" fontName="OpenSans" fontSize="9" leftIndent="18" rightIndent="18" spaceBefore="7" leading="12" textColor="#757474" />
  </stylesheet>
  <story>
    <keepInFrame frame="logo" onOverflow="shrink">
      <blockTable metal:use-macro="macro:brand-logo" rowHeights="612" colWidths="187" style="simple" />
    </keepInFrame>

    <keepInFrame frame="heading" onOverflow="shrink">
      <h3 style="header">
        <omitted tal:replace="options/Brand"/>
        <div tal:omit-tag="" tal:content="python:options.get('CertificateLabel') or default">Certificate of Completion</div>
      </h3>
    <h1 style="name" tal:content="options/Name">Huey Sooner</h1>
    <para style="license_line">LICENSE #:   _____________________________________________________</para>
    <h3 style="header"><omitted tal:replace="options/Brand">Janux</omitted> Course</h3>
    <h2 style="course" tal:content="options/Course">Course Title</h2>
    <h5 style="date">Awarded on <ommitted tal:replace="options/Date">March 19, 2008</ommitted></h5>

    <tal:block tal:define="units python: options['Credit'][0]['Units']"
               tal:condition="python: bool(len(options['Credit']) == 1)">
        <para style="section-header" spaceBefore="43" >Administrators and Operators:</para>
        <para style="section-info">
        This course is approved for ${units} continuing education credit hour(s)
        for adult care home administrators and operators, in the core area of
        Administration by the Kansas Department Aging and Disability Services.
        KDADS Health Occupations Credentialing sponsorship number LTS-A0001.
        </para>
        <para style="section-header">Nurses:</para>
        <para style="section-info">
        The Kansas Health Care Association is approved as a provider of
        continuing nursing education by the Kansas State Board of Nursing.
        This program offering is approved for ${units} contact hour(s)
        applicable for RN, LPN, and LMHT relicensure. Kansas State Board of
        Nursing Approved Provider Number: LT0030-0338.
        </para>
        <para style="section-header">Social Workers:</para>
        <para style="section-info">
        Kansas Health Care Association is an approved provider through the
        Kansas Behavioral Sciences Regulatory Board of continuing education for
         social workers. KSBSRB Approval 17-006. This course is approved for
         ${units} continuing education hour(s).
        </para>
        <para style="section-header">Dietary:</para>
        <para style="section-info">
        This course is approved for ${units} hour(s) of continuing education
        hour(s) for Kansas licensed dietitians by the Kansas Department of
        Aging and Disability Services Health Occupations Credentialing.
        Sponsorship number: LTS- D0002. CDMs - please consult your certifying
        board for approval.
        </para>
        <para style="section-header">Other:</para>
        <para style="section-info">
        Kansas HealthCare Association is an approved provider of continuing
        education for many health care disciplines. Please consult your
        licensing board for approval.
        </para>
    </tal:block>
    </keepInFrame>
  </story>
</document>
